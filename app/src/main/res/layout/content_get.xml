<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cn.wjh1119.webprogramming.GetActivity"
    tools:showIn="@layout/activity_get"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btn_get_httpurlconnection"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/btntext_get_httpurlconnection"
        tools:text="建立连接"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_get_retrofit" />

    <Button
        android:id="@+id/btn_get_okhttp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/btntext_get_okhttp"
        tools:text="建立连接"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_get_httpurlconnection"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btn_get_retrofit"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="@string/btntext_get_retrofit"
        tools:text="建立连接"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView_get"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="8dp"
        android:text="start \n"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_get_okhttp"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>
